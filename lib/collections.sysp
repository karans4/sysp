;; collections.sysp â€” generic collection operations
;; Vector/HashMap types are compiler builtins (need malloc/realloc).
;; Access operations live here as library poly-fns.

(defn len [x :?] :int
  (get x len))

(defn vector-ref [v :? i :int] :?
  (array-ref (get v data) i))

(defn vector-set! [v :? i :int val :?] :?
  (array-set! (get v data) i val))
